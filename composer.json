{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "binafy/laravel-user-monitoring": "^1.1",
        "botman/botman": "^2.8",
        "botman/driver-telegram": "^2.0",
        "botman/driver-web": "^1.5",
        "endroid/qr-code": "^5.0",
        "fakerphp/faker": "^1.24",
        "geoip2/geoip2": "^3.0",
        "graham-campbell/markdown": "^15.2",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^3.11",
        "irazasyed/telegram-bot-sdk": "^3.14",
        "itmm/m-mart-skin": "^1.0",
        "khanamiryan/qrcode-detector-decoder": "^2.0",
        "ladumor/laravel-pwa": "^0.0.4",
        "laravel/framework": "^10.0",
        "laravel/passport": "^12.4",
        "laravel/pulse": "^1.2",
        "laravel/sanctum": "^3.2",
        "laravel/socialite": "^5.17",
        "laravel/tinker": "^2.8",
        "league/flysystem-aws-s3-v3": "^3.29",
        "livewire/livewire": "^3.5",
        "maatwebsite/excel": "^3.1",
        "munafio/chatify": "^1.6",
        "predis/predis": "^2.3",
        "pusher/pusher-php-server": "^7.2",
        "santigarcor/laratrust": "^8.3",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "smalot/pdfparser": "^2.11",
        "spatie/laravel-activitylog": "^4.8",
        "spatie/laravel-server-monitor": "^1.10",
        "spatie/pdf-to-image": "^1.2",
        "torann/geoip": "^3.0",
        "tymon/jwt-auth": "^2.1",
        "unisharp/laravel-filemanager": "^2.9",
        "vinkla/hashids": "^11.0",
        "yajra/laravel-datatables-oracle": "^10.11",
        "yaza/laravel-repository-service": "^5.1"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.x-dev"
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
